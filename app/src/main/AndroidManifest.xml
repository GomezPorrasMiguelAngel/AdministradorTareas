<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.herprogramacion.lawyersapp">


    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".lawyers.MateriaActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".lawyerdetail.MateriaDetailActivity"
            android:label="@string/title_activity_lawyer_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".addeditlawyer.AddEditMateriaActivity"
            android:label="@string/title_activity_add_edit"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity
            android:name=".lawyers.TareaActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".lawyerdetail.TareaDetailActivity"
            android:label="@string/title_activity_lawyer_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".addeditlawyer.AddEditTareaActivity"
            android:label="@string/title_activity_add_edit"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
    </application>

</manifest>